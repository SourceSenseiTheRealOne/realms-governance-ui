# Solana RPC Endpoints
# You can use public endpoints or get your own from providers like Helius, QuickNode, or Alchemy
MAINNET_RPC=https://api.mainnet-beta.solana.com
DEVNET_RPC=https://api.devnet.solana.com

# Alternative: Use NEXT_PUBLIC_ prefix to make them available in the browser
NEXT_PUBLIC_MAINNET_RPC=https://api.mainnet-beta.solana.com
NEXT_PUBLIC_DEVNET_RPC=https://api.devnet.solana.com

# Helius RPC (Optional - for enhanced NFT/DAS features)
# Get your API key from https://helius.dev
NEXT_PUBLIC_HELIUS_MAINNET_RPC=
NEXT_PUBLIC_HELIUS_DEVNET_RPC=

# Backend RPC (for API routes)
BACKEND_MAINNET_RPC=https://api.mainnet-beta.solana.com

# Governance Program ID (Optional)
DEFAULT_GOVERNANCE_PROGRAM_ID=GTesTBiEWE32WHXXE2S4XbZvA5CrEc4xs6ZgRe895dP

# Realm Configuration (Optional - to run in single realm mode)
# REALM=MNGO

# Jupiter Swap API
NEXT_PUBLIC_JUPTER_SWAP_API_ENDPOINT=https://quote-api.jup.ag/v6

# Realms API
NEXT_PUBLIC_API_ENDPOINT=https://api.realms.today/graphql

# Discord Integration (Optional)
NEXT_PUBLIC_DISCORD_APPLICATION_CLIENT_ID=
NEXT_PUBLIC_DISCORD_MATCHDAY_CLIENT_ID=

# Sentry Error Tracking (Optional)
SENTRY_DSN=
SENTRY_AUTH_TOKEN=
NEXT_PUBLIC_SENTRY_DSN=

# Feature Flags (Optional)
DISABLE_NFTS=false
MAIN_VIEW_SHOW_MAX_TOP_TOKENS_NUM=

# Vercel Environment (Optional - Railway will set this automatically)
VERCEL_ENV=production

